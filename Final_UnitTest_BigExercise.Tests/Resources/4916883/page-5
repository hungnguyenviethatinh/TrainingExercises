                    'tip.state.unsubscribed': "Subscribe to notifications",
                    'tip.state.subscribed': "You\'re subscribed to notifications",
                    'tip.state.blocked': "You\'ve blocked notifications",
                    'message.prenotify': "Click to subscribe to notifications",
                    'message.action.subscribed': "Thanks for subscribing!",
                    'message.action.resubscribed': "You\'re subscribed to notifications",
                    'message.action.unsubscribed': "You won\'t receive notifications again",
                    'dialog.main.title': "Manage Site Notifications",
                    'dialog.main.button.subscribe': "SUBSCRIBE",
                    'dialog.main.button.unsubscribe': "UNSUBSCRIBE",
                    'dialog.blocked.title': "Unblock Notifications",
                    'dialog.blocked.message': "Follow these instructions to allow notifications:"
                }
            },
            persistNotification: false
        }]);
        OneSignal.push(function () {


            var platform = 'desktop';
            if (navigator.userAgent.match(/Android|webOS|iOS|iPhone|iPad|iPod|BlackBerry|WPDesktop|IEMobile|Opera Mini|Windows Phone|Mobile Safari/i)) {
                platform = 'mobile';
            }

            OneSignal.on('subscriptionChange', function (isSubscribed) {
                if (isSubscribed) {
                    window.setTimeout(function () {
                        OneSignal.sendTags({
                            platform: platform,
                            user_id: 0,
                        });
                    }, 3000);
                }
            });
            OneSignal.getTags().then(function (tags) {
                if (tags === null || !tags) {
                    OneSignal.sendTags({
                        platform: platform,
                        user_id: 0,
                    });
                } else {
                    if (typeof (tags.user_id) == 'undefined' || !(tags.user_id) || tags.user_id === '0') {
                        OneSignal.sendTag("user_id", "0");
                    }
                    if (typeof (tags.platform) == 'undefined' || !(tags.platform)) {
                        OneSignal.sendTag('platform', platform);
                    }
                }
            });
        });
    </script>
